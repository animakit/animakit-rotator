{
  "name": "animakit-rotator",
  "version": "1.0.3",
  "description": "AnimakitRotator - React component for three-dimensional rotation of content blocks",
  "main": "dist/index.js",
  "files": [
    "*.md",
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack": "cross-env NODE_ENV=production webpack",
