{
  "name": "animakit-rotator",
  "version": "2.0.0-beta",
  "description": "AnimakitRotator - React component for the 3D rotation of the blocks",
  "main": "index.js",
  "files": [
    "*.md",
    "lib/*.js"
  ],
  "scripts": {
    "clean": "rimraf lib/*.js",
    "build": "npm run clean && babel -s inline -d ./ **/*.es6",
    "prepublish": "npm run build",
    "test": "ava"
  },
  "author": "Anna Selezniova",
  "license": "MIT",
  "keywords": [
    "animakit",
    "animation",
    "rotator",
    "rotation",
    "react",
    "react-component"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/animakit/animakit-rotator"
  },
  "homepage": "https://animakit.github.io",
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.16.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.16.3",
    "enzyme": "^2.4.1",
    "jsdom": "^9.6.0",
    "react-addons-test-utils": "^15.3.2",
    "rimraf": "^2.5.4"
  },
  "dependencies": {
    "animakit-core": "^2.0.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2"
  },
  "ava": {
    "files": [
      "test/**/*-test.js"
    ],
    "require": [
      "babel-register",
      "./test/.setup.js"
    ],
    "babel": "inherit"
  }
}
