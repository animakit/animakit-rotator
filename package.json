    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.1.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "cross-env": "^1.0.6",
    "enzyme": "^2.2.0",
    "jsdom": "^8.4.0",
    "mocha": "^2.4.5",
    "react-addons-test-utils": "^15.0.1",
    "rimraf": "^2.5.2",
    "webpack": "^1.13.0"
  },
  "dependencies": {
